---
import DocClass from "../../components/documentation/DocClass.astro"
import Layout from "../../layouts/Layout.astro";
import data from "/public/JSON/was.documentation.json"

import { Accordion } from 'astro-bootstrap';
const id = 'manualAccordion'

console.log(Object.keys(data[0]));


---

<Layout>
<Accordion id={id}>
    {data.map((classData: any, i: number)=>(
        <DocClass {classData} classId={i} parentId={id}></DocClass>
    ))}
</Accordion>
</Layout>
