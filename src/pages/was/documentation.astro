const items = [
  { header: 'Title 1', body: 'Text block 1', show: true },
  { header: 'Title 2', body: 'Text block 2' },
  { header: 'Title 3', body: 'Text block 3' },
];
const accordionId = 'accordionExample';

import { Accordion } from 'astro-bootstrap';
